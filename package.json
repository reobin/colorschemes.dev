  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "start::local": "npm run develop -- -H 0.0.0.0",
    "clean": "gatsby clean",
    "seed": "sh db/seed.sh",
    "preview::build": "npm run clean && npm run build",
