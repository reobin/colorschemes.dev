    "test": "jest --silent",
    "test::vocal": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reobin/vimcolorschemes"
  },
  "bugs": {
    "url": "https://github.com/reobin/vimcolorschemes/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md,css,scss}": "prettier --write",
    "*.{js,jsx}": "jest --findRelatedTests"
  }
